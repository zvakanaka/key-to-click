{
	"manifest_version": 2,
	"name": "Key to Click",
	"description": "Assign keys to click for you",
	"version": "0.0.1",
	"web_accessible_resources": [
		"popup.js"
	],
	"icons": {
		"48": "icon.png",
		"96": "icon@2x.png"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"node_modules/webextension-polyfill/dist/browser-polyfill.js",
				"content_script.js"
			],
			"all_frames": true,
			"run_at": "document_end"
		}
	],
	"permissions": [
		"tabs",
		"storage"
	],
	"browser_action": {
		"default_icon": {
			"96": "icon@2x.png"
		},
		"default_popup": "browserAction/index.html",
		"default_title": "Key to Click"
	},
	"options_ui": {
		"page": "options.html",
		"browser_style": true,
		"chrome_style": true
	}
}