{
	"manifest_version": 3,
	"name": "Key to Click",
	"description": "Assign keys to click for you",
	"version": "0.1.0",
	"web_accessible_resources": [{
		"resources": [
			"popup.js"
		],
		"matches": [],
		"extension_ids": []
	}
	],
	"icons": {
		"48": "icons/icon.png",
		"96": "icons/icon@2x.png"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"vendor/browser-polyfill.min.js",
				"content_script.js"
			],
			"all_frames": true,
			"run_at": "document_end"
		}
	],
	"permissions": [
		"tabs",
		"storage"
	],
	"action": {
		"default_icon": {
			"96": "icons/icon@2x.png"
		},
		"default_popup": "browserAction/index.html",
		"default_title": "Key to Click"
	},
	"options_ui": {
		"page": "options.html",
		"browser_style": true
	}
}